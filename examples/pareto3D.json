{
    "description": "Demo of pareto 3D front visualization",
    "base encoding": "#const max=4.  dom(1..max).  {m(1..max)} = 1.  a(1) :- m(1).  a(2) :- m(2).  a(3) :- m(3).  a(4) :- m(4).  b(3) :- m(1).  b(2) :- m(2).  b(1) :- m(3).  b(2) :- m(4).  c(2) :- m(3). #show a/1.  #show b/1. #show c/1.",

    "users options": {
        "type": "restricted",
        "allowed": ["A", "B", "C"]
    },
    "choices options": {
        "default": "all",
        "type": "multiple users"
    },
    "output options": {
        "plugin repr": [
            {
                "kind": "pareto front 3D",
                "title": "Optimal solutions",
                "x": "a",
                "y": "b",
                "z": "c",
                "x_label": "score of a/1 atom",
                "y_label": "score of b/1 atom",
                "z_label": "score of c/1 atom",
                "in": "header"
            }
        ]
    }
}
